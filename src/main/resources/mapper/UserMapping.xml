<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tReAsures.demo.dao.UserDao" >
	<select id="getUserName" parameterType="String" resultType="com.tReAsures.demo.entity.User">
		select 
			* 
		from  user
		where username = #{value}
	</select>
</mapper>